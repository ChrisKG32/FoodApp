template(name='Shop')
	.shop.transitions-content
		+MiniNav
		if aisles
			.smart-list.container
				table.table
					each aisles
						thead
							tr
								th
									img(src="!{this.icon}")
									| !{this.name}
						tbody
							each generateList
								tr
									td
										| !{this.amount} !{this.measurement} !{this.name}

							